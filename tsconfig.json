{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "./dist", //生成的js和js.map的目录
        // 支持装饰器
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
    },
    "include": [ // 需要编译的ts文件一个*表示文件匹配**表示忽略文件的深度问题
        "./*.ts",
        "./**/*.ts"
    ],
}